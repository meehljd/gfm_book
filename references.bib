}

@article{ung_multiorgan_2024,
	title = {Multiorgan locked-state model of chronic diseases and systems pharmacology opportunities},
	volume = {29},
	issn = {1878-5832},
	doi = {10.1016/j.drudis.2023.103825},
	abstract = {With increasing human life expectancy, the global medical burden of chronic diseases is growing. Hence, chronic diseases are a pressing health concern and will continue to be in decades to come. Chronic diseases often involve multiple malfunctioning organs in the body. An imminent question is how interorgan crosstalk contributes to the etiology of chronic diseases. We conceived the locked-state model (LoSM), which illustrates how interorgan communication can give rise to body-wide memory-like properties that 'lock' healthy or pathological conditions. Next, we propose cutting-edge systems biology and artificial intelligence strategies to decipher chronic multiorgan locked states. Finally, we discuss the clinical implications of the LoSM and assess the power of systems-based therapies to dismantle pathological multiorgan locked states while improving treatments for chronic diseases.},
	language = {eng},
	number = {1},
	journal = {Drug Discovery Today},
	author = {Ung, Choong Yong and Correia, Cristina and Li, Hu and Adams, Christopher M. and Westendorf, Jennifer J. and Zhu, Shizhen},
