	pages = {296--305},
	file = {Full Text PDF:/Users/meehl.joshua/Zotero/storage/ITB5QUND/2021-01 - Zeng et al. - A Deep Learning Framework for Identifying Essential Proteins by Integrating Multiple Types of Biolog.pdf:application/pdf},
}

@article{schapke_epgat_2022,
	title = {{EPGAT}: {Gene} {Essentiality} {Prediction} {With} {Graph} {Attention} {Networks}},
