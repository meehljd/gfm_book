	pages = {1034--1050},
	file = {Full Text:/Users/meehl.joshua/Zotero/storage/B4H9S59F/2005-08 - Siepel et al. - Evolutionarily conserved elements in vertebrate, insect, worm, and yeast genomes.pdf:application/pdf},
}

