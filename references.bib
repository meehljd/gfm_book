@article{zeng_deep_2021,
	title = {A {Deep} {Learning} {Framework} for {Identifying} {Essential} {Proteins} by {Integrating} {Multiple} {Types} of {Biological} {Information}},
	volume = {18},
	issn = {1557-9964},
