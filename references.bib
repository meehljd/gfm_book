	pages = {1863--1869},
	file = {ScienceDirect Full Text PDF:/Users/meehl.joshua/Zotero/storage/J2FQDK87/2013-07-01 - Rogers et al. - Ectopic Expression of Zmiz1 Induces Cutaneous Squamous Cell Malignancies in a Mouse Model of Cancer.pdf:application/pdf;ScienceDirect Snapshot:/Users/meehl.joshua/Zotero/storage/9TN3EX7J/S0022202X15363260.html:text/html},
