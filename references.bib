	file = {Full Text PDF:/Users/meehl.joshua/Zotero/storage/Q6SJDLPT/2025-04-09 - Xu and Wu - Using machine learning and single nucleotide polymorphisms for improving rheumatoid arthritis risk P.pdf:application/pdf},
}

@misc{benegas_genomic_2024,
